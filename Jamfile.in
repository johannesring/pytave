# @configure_input@

project py : 
	requirements 
	<library>/usr/lib/lib@BOOST_PYTHON_LIB@.so  
	<library>@JAM_LIBOCTAVE@
	<library>@JAM_LIBCRUFT@
	<library>@JAM_LIBOCTINTERP@
